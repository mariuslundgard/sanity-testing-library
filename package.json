{
  "name": "sanity-testing-library",
  "version": "1.0.0",
  "description": "Testing utilities for Sanity.io",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/mariuslundgard/sanity-testing-library#readme",
  "bugs": {
    "url": "https://github.com/mariuslundgard/sanity-testing-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariuslundgard/sanity-testing-library.git"
  },
  "license": "MIT",
  "author": "Marius Lundg√•rd <marius@sanity.io>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "node": {
        "import": "./dist/index.cjs.js",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run-s clean pkg:build pkg:check",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint .",
    "pkg:build": "pkg-utils build --strict",
    "pkg:check": "pkg-utils --strict",
    "pkg:watch": "pkg-utils watch --strict",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "test": "run-s vitest:test",
    "test:browser": "echo 'TODO'",
    "type-check": "tsc --build",
    "vitest:test": "vitest run",
    "watch": "run-s pkg:watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.5",
    "get-it": "^8.1",
    "groq-js": "^1.1.9",
    "rxjs": "^7.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@sanity/client": "^5.4.2",
    "@sanity/pkg-utils": "^2.2.14",
    "@sanity/semantic-release-preset": "^4.1.1",
    "@sanity/ui": "^1.3.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "sanity": "^3.9.0",
    "semantic-release": "^21.0.1",
    "styled-components": "^5.3.10",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@sanity/client": "^5",
    "@sanity/ui": "^1",
    "react": "^18",
    "sanity": "^3",
    "styled-components": "^5.2"
  },
  "packageManager": "pnpm@8.3.1",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "rxjs": "^7.8.0"
    }
  }
}
